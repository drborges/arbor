{
  "name": "@arborjs/plugins",
  "version": "0.0.1-alpha.40",
  "sideEffects": false,
  "description": "Community driven plugins for @arborjs/store.",
  "keywords": [
    "arbor",
    "plugins",
    "react",
    "state",
    "store",
    "typescript",
    "tree"
  ],
  "repository": {
    "url": "https://github.com/drborges/arbor",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/drborges/arbor/issues",
    "email": "drborges.cic@gmail.com"
  },
  "author": {
    "email": "drborges.cic@gmail.com",
    "name": "Diego Borges",
    "url": "https://github.com/drborges"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "./dist",
    "./README",
    "../../LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@arborjs/store": "0.0.1-alpha.40",
    "@types/jest": "^26.0.23",
    "esbuild": "^0.12.12",
    "eslint": "^7.32.0",
    "fake-indexeddb": "^3.1.7",
    "idb": "^7.0.0",
    "jest": "^26.6.3",
    "np": "^7.5.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@arborjs/store": "0.0.1-alpha.40",
    "idb": "*"
  },
  "peerDependenciesMeta": {
    "idb": {
      "optional": true
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --passWithNoTests",
    "dev": "jest --watch",
    "build": "yarn clean && NODE_ENV=production ENTRYPOINTS=index.ts,IndexedDB.ts,LocalStorage.ts,Logger.ts node ../../tools/build.js && yarn tsc",
    "build:dev": "yarn clean && NODE_ENV=development ENTRYPOINTS=index.ts,IndexedDB.ts,LocalStorage.ts,Logger.ts node ../../tools/build.js && yarn tsc",
    "prepublish": "yarn build",
    "publish": "np"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.mjs"
    },
    "./Logger": {
      "import": "./dist/Logger.esm.js",
      "require": "./dist/Logger.cjs.js",
      "default": "./dist/Logger.mjs"
    },
    "./LocalStorage": {
      "import": "./dist/LocalStorage.esm.js",
      "require": "./dist/LocalStorage.cjs.js",
      "default": "./dist/LocalStorage.mjs"
    },
    "./idb": {
      "import": "./dist/IndexedDB.esm.js",
      "require": "./dist/IndexedDB.cjs.js",
      "default": "./dist/IndexedDB.mjs"
    }
  }
}
